{
  "search.exclude": {
    "**/dist/**/*": true,
    "**/static/**/*": true,
    // settingSearch
"src/fonts/**/*": true,
    "gulp_themes_config.js": true,
    "gulp_icons-color-config.js": true,"src/custom/css": true,"src/scss": true,
"theme/vietcombank": true,
// endSettingSearch
  },
  "files.exclude": {
    ".temp": true,
    "static": true,
    "static-theme": true,
  },
  "html.suggest.html5": true,
  "emmet.includeLanguages": {
    "nunjucks": "html"
  },
  "files.associations": {
    "*.njk": "nunjucks"
  },
  "editor.tabSize": 2,
  "scssFormatter.printWidth": 12000,
  "html.format.wrapLineLength": 1200,
  "html.format.wrapAttributes": "preserve",
  "workbench.editor.closeEmptyGroups": false,
  "editor.occurrencesHighlight": false,
  "path-autocomplete.pathMappings": {
    "media": "${folder}/src/media",
    "pages": "${folder}/src/pages",
    "custom": "${folder}/src/custom",
    "plugins": "${folder}/src/plugins",
    "src": "${folder}/src",
    "/": "${folder}"
  },
 "cssVariables.lookupFiles": [
    "src/custom/css/0-gen_varibles.css"
  ],
  "path-autocomplete.ignoredFilesPattern": "**/*.{css,scss,html}",
  "vscode-opn.perLang": {
    "opnOptions": [
      {
        "forLang": "html",
        "openInApp": "chrome",
        "isUseWebServer": true,
        "isUseFsPath": false,
        "isWaitForAppExit": true,
        "trimPath": "src/"
      },
      {
        "forLang": "nunjucks",
        "openInApp": "chrome",
        "isUseWebServer": true,
        "isUseFsPath": false,
        "isWaitForAppExit": true,
        "trimPath": "src/"
      }
    ]
  },
  "vscode-opn.webServerProtocol": "http",
  "vscode-opn.webServerHostname": "localhost",
  "vscode-opn.webServerPort": 3000,
  "workbench.colorCustomizations": {
    "statusBar.background": "#61dafb",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#2fcefa",
    "activityBar.activeBackground": "#93e6fc",
    "activityBar.activeBorder": "#fa45d4",
    "activityBar.background": "#93e6fc",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#fa45d4",
    "activityBarBadge.foreground": "#15202b",
    "titleBar.activeBackground": "#61dafb",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#61dafb99",
    "titleBar.inactiveForeground": "#15202b99",
    "editorGroup.border": "#93e6fc",
    "panel.border": "#93e6fc",
    "sash.hoverBorder": "#93e6fc",
    "sideBar.border": "#93e6fc",
    "statusBarItem.remoteBackground": "#61dafb",
    "statusBarItem.remoteForeground": "#15202b"
  },
  "peacock.color": "#61dafb"
}