{
  "search.exclude": {
    "**/dist/**/*": true
  },
  "html.suggest.html5": true,
  "emmet.includeLanguages": {
    "nunjucks": "html"
  },
  "files.associations": {
    "*.njk": "nunjucks"
  },
  "editor.tabSize": 2,
  "scssFormatter.printWidth": 12000,
  "html.format.wrapLineLength": 1200,
  "html.format.wrapAttributes": "preserve",
  "workbench.editor.closeEmptyGroups": false,
  "editor.occurrencesHighlight": false,
  "path-autocomplete.pathMappings": {
    "media": "${folder}/src/media",
    "custom": "${folder}/src/custom",
    "plugins": "${folder}/src/plugins",
    "src": "${folder}/src",
    "/": "${folder}"
  },
  "path-autocomplete.ignoredFilesPattern": "**/*.{css,scss,html}",
  "vscode-opn.perLang": {
    "opnOptions": [
      {
        "forLang": "html",
        "openInApp": "chrome",
        "isUseWebServer": true,
        "isUseFsPath": false,
        "isWaitForAppExit": true,
        "trimPath": "src/"
      },
      {
        "forLang": "nunjucks",
        "openInApp": "chrome",
        "isUseWebServer": true,
        "isUseFsPath": false,
        "isWaitForAppExit": true,
        "trimPath": "src/"
      }
    ]
  },
  "vscode-opn.webServerProtocol": "http",
  "vscode-opn.webServerHostname": "localhost",
  "vscode-opn.webServerPort": 3000,
  "workbench.colorCustomizations": {
    "editor.background": "#171c20",
    "editor.findMatchBackground": "#ff008056",
    "sideBar.background": "#171c20",
    "editor.lineHighlightBackground": "#171c20",
    "editorIndentGuide.background": "#333333",
    "editorBracketMatch.border": "#171c20",
    "editorBracketMatch.background": "#424242",
    "activityBar.background": "#0E2E53",
    "titleBar.activeBackground": "#134074",
    "titleBar.activeForeground": "#F7FAFE"
  }
}